Theorem eq_example1 : forall (A B C : Type) (f : A -> B) (g : B -> C) (x : A) (y : B),
    y = f x -> g y = g (f x).

Proof.
(* The theorem is a simple functional equality.
We will proceed by induction on the equality y = f x.
*)
  intros A B C f g x y H.
(* We first consider the base case where y = f x.
In this case, the theorem is trivially true.
*)
  subst H.
reflexivity.
(* Now we consider the inductive case where y <> f x.
In this case, we can rewrite the goal using the definition of g.
*)
  rewrite H.
(* Since g y = g (f x) by the inductive hypothesis, the theorem is true in this case as well.
*)
  reflexivity.
Qed.

