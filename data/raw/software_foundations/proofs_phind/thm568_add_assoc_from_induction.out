Lemma addition_of_zero_1 : forall n p : nat, n + p = n + 0 + p.

Proof.
(* The goal is to prove that n + p = n + 0 + p.
We will proceed by induction on n.
*)
  intros n p.
(* First, we need to define the induction hypothesis.
*)
  induction n as [|n' IHn'].
(* The base case is when n = 0.
In this case, n + p = 0 + p by definition of addition.
*)
  - simpl.
reflexivity.
(* The inductive case is when n = S n'.
We will use the induction hypothesis IHn' on n'.
*)
  - simpl.
rewrite IHn'.
reflexivity.
Qed.

Lemma addition_of_zero_0 : forall n : nat, forall p : nat, n + p = n + 0 + p.

Proof.
(* The theorem to be proven is an equality between two expressions involving natural numbers.
The goal is to prove that these two expressions are equal.
*)
  intros n p.
(* We first perform a computation step by simplifying the left-hand side of the equality.
*)
  simpl.
(* The left-hand side simplifies to "n + p".
*)
  (* Now, we perform a computation step on the right-hand side of the equality.
*)
  simpl.
(* The right-hand side simplifies to "n + 0 + p", which is equal to "n + p" by the associativity of addition.
*)
  (* We can now conclude the proof by reflexivity.
*)
  apply (@addition_of_zero_1 n p).
Qed.

Lemma addition_associativity_lemma_3 : forall n m' : nat, forall IH_m' : forall p : nat, n + (m' + p) = n + m' + p, forall p : nat, n + S (m' + p) = n + S m' + p.

Proof.
(*
    We will proceed by induction on m'.
The base case will be when m' = 0.
The inductive case will be when m' = S m.
*)
  intros n m' IH_m' p.
(*
    We will first show that n + (m' + p) = n + m' + p.
This is done by applying the induction hypothesis IH_m'.
*)
  rewrite IH_m'.
(*
    Now we will show that n + S (m' + p) = n + S m' + p.
We will do this by applying the induction hypothesis IH_m' to the term S (m' + p).
*)
  rewrite (IH_m' (S p)).
(*
    The goal is now n + S (m' + p) = n + S m' + S p.
We will now rewrite this goal to n + (m' + S p) = n + m' + S p,
    which is the same as n + S (m' + p) = n + S m' + p,
    and thus we have shown the lemma.
*)
  reflexivity.
Qed.

