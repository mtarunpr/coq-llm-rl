Example In_example_2 : forall n, In n [2; 4] ->
  exists n', n = 2 * n'.

Proof.
(* We will prove the theorem by induction on n.
*)
  intros n H.
(* We will first consider the case where n = 2.
*)
  induction n as [|n' IHn'].
- (* Base case: n = 0.
*)
    simpl.
(* We know that In 0 [2; 4] is false, so we can use ex_falso_quodlibet to derive a contradiction.
*)
    apply (ex_falso_quodlibet _ H).
(* This shows that n = 0 is impossible.
*)
    intros contra.
(* Now we consider the case where n = S n'.
*)
    (* We know that In (S n') [2; 4] is also false, so we can again use ex_falso_quodlibet to derive a contradiction.
*)
    apply (ex_falso_quodlibet _ H).
(* This shows that n = S n' is impossible.
*)
    intros contra'.
(* Since we have shown that both n = 0 and n = S n' are impossible, we can use the law of the excluded middle to derive a contradiction.
*)
    apply (ex_falso_quodlibet _ (or_intro_l _ _ contra)).
(* This completes the proof.
*)
  
Qed.

