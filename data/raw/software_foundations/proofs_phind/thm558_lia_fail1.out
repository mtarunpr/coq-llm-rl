Theorem lia_fail1 : 0 = 1.

Proof.
(* This theorem is false, as 0 is not equal to 1.
We can prove this by contradiction.
Assume the negation of the theorem, i.e., 0 <> 1.
*)
  intros contra.
(* Now we have the context: contra : 0 <> 1 *)
  (* Since 0 = 0 by reflexivity, we can now apply the contra to show that 0 = 1, which is a contradiction.
*)
  apply contra in H.
(* Now we have the context: H : 0 = 1 *)
  (* We can now derive the theorem by applying the contradiction.
*)
  exact H.
Qed.

