Theorem eq_implies_succ_equal : forall (n m : nat),
  n = m -> S n = S m.

Proof.
(* The theorem states that if n is equal to m, then the successor of n is equal to the successor of m.
To prove this, we will proceed by induction on n.
*)
  intros n m.
(* The base case is when n is 0.
In this case, we have n = m, so S n = S m.
*)
  induction n as [|n' IHn'].
- intros m' eq_n_m'.
(* We have n = 0 and m = 0, so S 0 = S 0.
*)
    simpl.
reflexivity.
(* The inductive case is when n is the successor of some n'.
We have n' = m' by the induction hypothesis.
We need to prove that S n' = S m'.
*)
  intros m' eq_n_m'.
(* By the induction hypothesis, we have IHn' : n' = m'.
We can now rewrite eq_n_m' with this equality to get S n' = S m'.
*)
  rewrite (IHn' eq_n_m').
(* This concludes the proof.
*)
  reflexivity.
Qed.

