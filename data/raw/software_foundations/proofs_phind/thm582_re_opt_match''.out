Lemma regex_optional_match_2 : forall T : Type, forall re : reg_exp T, forall s : list T, forall H : s =~ re, s =~ re_opt re.

Proof.
(* We will proceed by induction on the structure of the regular expression re.
*)
  intros T re s H.
induction re as [|re1 re2 Hre1 Hre2].
(* Case re = EmptySet: *)
  - simpl.
(* Since s =~ EmptySet is false, we have a contradiction.
*)
    contradiction.
(* Case re = Epsilon: *)
  - simpl.
(* Since s =~ Epsilon is false, we have a contradiction.
*)
    contradiction.
(* Case re = Char c: *)
  - simpl.
(* Since s =~ Char c is false, we have a contradiction.
*)
    contradiction.
(* Case re = Star re1: *)
  - simpl.
(* Since s =~ Star re1 is false, we have a contradiction.
*)
    contradiction.
(* Case re = re1 + re2: *)
  - simpl.
(* We know that s =~ re1 + re2.
*)
    (* We need to prove that s =~ re_opt (re1 + re2).
*)
    (* By definition, re_opt (re1 + re2) = re_opt re1 + re_opt re2.
*)
    (* We know that s =~ re1 + re2, so s =~ re_opt re1 + re_opt re2.
*)
    (* Therefore, s =~ re_opt (re1 + re2).
*)
    reflexivity.
(* Case re = re1 * re2: *)
  - simpl.
(* Since s =~ re1 * re2 is false, we have a contradiction.
*)
    contradiction.
Qed.

