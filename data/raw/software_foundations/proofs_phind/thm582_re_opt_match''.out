Lemma regex_optional_match_0 : forall T : Type, forall re : reg_exp T, forall s : list T, forall H : s =~ re, s =~ re_opt re.

Proof.
(*
     We will proceed by induction on the structure of the regular expression re.
The base case will be when re is an empty regular expression, in which case the result is trivial.
The inductive case will be when re is a concatenation of two regular expressions, re1 and re2.
We will then proceed by cases on whether s matches re1, and prove the lemma in each case.
*)
  intros T re s H.
induction re as [|re1 re2 Hre1re2].
- (* re = ε *)
    (* In this case, the empty string matches the empty regular expression, and the optional regular expression is also the empty regular expression.
*)
    simpl.
(* We need to prove s =~ ε, which is trivial since ε only matches the empty string.
*)
    reflexivity.
- (* re = re1 ++ re2 *)
    (* We will proceed by cases on whether s matches re1.
*)
    destruct (s =~ re1) as [Hre1 | Hnsre1].
- (* s =~ re1 *)
      (* In this case, we need to prove s =~ (re1 ++ re2).
*)
      simpl in H.
(* We know that s =~ re1 ++ re2, since s =~ re1 and s =~ re2.
*)
      left.
reflexivity.
- (* s ≁ re1 *)
      (* In this case, we need to prove s =~ (re1 ++ re2).
*)
      simpl in H.
(* We know that s =~ re1 ++ re2, since s =~ re1 and s =~ re2.
*)
      right.
reflexivity.
Qed.

