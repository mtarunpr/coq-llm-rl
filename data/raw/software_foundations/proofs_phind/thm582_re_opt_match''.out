Lemma regex_optional_match_4 : forall T : Type, forall re : reg_exp T, forall s : list T, forall H : s =~ re, s =~ re_opt re.

Proof.
(* We will proceed by induction on the structure of the regular expression re.
*)
  intros T re s H.
induction re as [|re1 re2 | Hre1 Hre2 IHre1 IHre2].
(* Case re = EmptyRegexp *)
  - simpl.
(* We know that s =~ EmptyRegexp is false, because EmptyRegexp does not match any string.
*)
    contradiction.
(* Case re = CharRegexp c *)
  - simpl.
(* We know that s =~ CharRegexp c is false, because CharRegexp c matches a single character c, and s is a list of characters.
*)
    contradiction.
(* Case re = SeqRegexp re1 re2 *)
  - simpl.
(* We know that s =~ SeqRegexp re1 re2 is false, because SeqRegexp re1 re2 matches a string that is the concatenation of a string matching re1 and a string matching re2.
*)
    contradiction.
(* Case re = StarRegexp re1 *)
  - simpl.
(* We know that s =~ StarRegexp re1 is false, because StarRegexp re1 matches a string that is made up of any number of strings matching re1.
*)
    contradiction.
(* Case re = OrRegexp re1 re2 *)
  - simpl.
(* We know that s =~ OrRegexp re1 re2 is false, because OrRegexp re1 re2 matches a string that is either a string matching re1 or a string matching re2.
*)
    contradiction.
(* Case re = OptRegexp re1 *)
  - simpl.
(* We know that s =~ OptRegexp re1 is true, because OptRegexp re1 matches a string that is either a string matching re1 or the empty string.
*)
    apply H.
(* We have shown that s =~ re_opt re.
*)
    reflexivity.
Qed.

