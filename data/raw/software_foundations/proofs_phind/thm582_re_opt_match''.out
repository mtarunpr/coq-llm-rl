Lemma regex_optional_match_1 : forall T : Type, forall re : reg_exp T, forall s : list T, forall H : s =~ re, s =~ re_opt re.

Proof.
(*
     We will proceed by induction on the structure of the regular expression re.
The case for the empty regular expression is trivial, as the empty regular expression
     only matches the empty string.
*)
  intros T re s H.
induction re as [|re1 re2 Hre1 Hre2].
- (* re = ε *)
    intros H.
simpl in H.
inversion H.
subst.
apply Hre1.
reflexivity.
- (* re = re1 | re2 *)
    intros H.
destruct H as [Hre1' | Hre2'].
- (* re = re1 | re2, s =~ re1 *)
      apply Hre1.
assumption.
- (* re = re1 | re2, s =~ re2 *)
      apply Hre2.
assumption.
Qed.

