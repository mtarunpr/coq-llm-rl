Lemma split_regex_match_4 : forall T : Type, forall re : reg_exp T, forall s : list T, forall H_match : s =~ re, exists s1 s2 s3 : list T,
  s = s1 ++ s2 ++ s3 /\
  s2 <> [ ] /\
  (forall m : nat, s1 ++ napp m s2 ++ s3 =~ re).

Proof.
(*
    We will proceed by induction on the structure of the regular expression re.
The base case will be when re is a single character, in which case we can directly split s into s1, s2, and s3.
The inductive case will be when re is a concatenation of two regular expressions, re1 and re2.
We will use the induction hypothesis to show that s1 ++ s2 ++ s3 matches re1, and then recursively apply the lemma to re2.
*)
  intros T re s H_match.
induction re as [|re1 re2 Hre2].
- (* re is a single character *)
    destruct s as [|s1 s2 s3].
+ (* s is empty *)
      exists s1, s2, s3.
split.
+ reflexivity.
+ intros m.
rewrite H_match.
reflexivity.
+ (* s is not empty *)
      exists s1, s2, s3.
split.
+ reflexivity.
+ intros m.
rewrite H_match.
reflexivity.
- (* re is a concatenation of re1 and re2 *)
    destruct s as [|s1 s2 s3].
+ (* s is empty *)
      inversion H_match.
+ (* s is not empty *)
      destruct (H_match (s1 ++ s2)) as [H_match1 H_match2].
destruct H_match2 as [s2' s3'].
exists s1, s2', s3'.
split.
+ reflexivity.
+ intros m.
rewrite H_match1.
apply IHre2 in H_match1.
rewrite H_match1.
reflexivity.
Qed.

