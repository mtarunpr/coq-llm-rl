Lemma split_regex_match_1 : forall T : Type, forall re : reg_exp T, forall s : list T, forall H_match : s =~ re, exists s1 s2 s3 : list T,
  s = s1 ++ s2 ++ s3 /\
  s2 <> [ ] /\
  (forall m : nat, s1 ++ napp m s2 ++ s3 =~ re).

Proof.
(*
    We will proceed by induction on the structure of the regular expression re.
The base case will be when re is a single character, in which case we can directly split s into s1, s2, and s3.
The inductive case will be when re is a concatenation of two regular expressions, re1 and re2.
We will use the induction hypothesis to show that s1 ++ s2 ++ s3 =~ re1 and s1 ++ s2 ++ s3 =~ re2.
Then, we will show that s =~ re.
*)
  intros T re s H_match.
induction re as [|re1 re2 Hre1_re2_re].
- (* re is a single character *)
    destruct s as [|s1 s2 s3 Hs1_s2_s3].
exists s1.
exists s2.
exists s3.
split.
- (* s = s1 ++ s2 ++ s3 *)
      reflexivity.
- (* s2 <> [] *)
      intro H_s2_eq_nil.
inversion H_s2_eq_nil.
- (* forall m, s1 ++ napp m s2 ++ s3 =~ re *)
      intros m.
rewrite Hs1_s2_s3.
reflexivity.
- (* re is a concatenation of re1 and re2 *)
    destruct H_match as [s1 s2 s3 Hs1_s2_s3 Hs1_s2_s3_match].
exists s1.
exists s2.
exists (s3 ++ s2).
split.
- (* s = s1 ++ s2 ++ (s3 ++ s2) *)
      reflexivity.
- (* s2 <> [] *)
      intro H_s2_eq_nil.
inversion H_s2_eq_nil.
- (* forall m, s1 ++ napp m s2 ++ (s3 ++ s2) =~ re *)
      intros m.
rewrite Hs1_s2_s3_match.
apply
Qed.

