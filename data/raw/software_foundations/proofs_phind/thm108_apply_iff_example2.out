Lemma logical_equivalence_implies_consequence_0 : forall P Q R : Prop, P <-> Q -> (P -> R) -> Q -> R.

Proof.
(* We will prove the lemma by applying the given logical equivalence P <-> Q to the premise P and the conclusion R.
*)
  intros P Q R H P_implies_R Q_implies_R.
(* We will first show that P implies R.
We can do this by applying the implication P -> R given as a premise.
*)
  apply (P_implies_R P).
(* Now, we will show that Q implies R.
We can do this by applying the implication Q -> R given as a premise.
*)
  apply (Q_implies_R Q).
(* We have now shown that P implies R and Q implies R.
By the logical equivalence P <-> Q, we can conclude that P implies R.
*)
  apply H.
(* We have now proven the lemma.
*)

Qed.

