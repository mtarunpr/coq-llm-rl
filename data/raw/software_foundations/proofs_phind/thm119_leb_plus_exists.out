Theorem leb_plus_exists : forall n m, n <=? m = true -> exists x, m = n+x.

Proof.
(*
    We will prove the theorem by induction on n.
The base case will be when n = 0.
In this case, we have n <=? m = true, which means m >= 0.
So, we can take x = m, and we have m = 0 + x, which is true.
*)
  intros n m H.
induction n as [|n' IHn'].
- (* n = 0 *)
    exists m.
reflexivity.
- (* n = S n' *)
    (*
      We have n' <=? m = true.
By the induction hypothesis, we have exists x, m = n' + x.
We will show that exists x, S (n' + x) = m.
*)
    destruct IHn' as [x [Hx1 Hx2]].
exists (S x).
simpl.
rewrite Hx2.
reflexivity.
Qed.

