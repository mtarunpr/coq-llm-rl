Lemma lemma_n_less_equal_m_exists_x_4 : forall n' m : nat, forall H : (S n' <=? m) = true, forall IHn' : (n' <=? m) = true -> exists x : nat, m = n' + x, exists x : nat, m = S n' + x.

Proof.
(* We will proceed by induction on n'.
*)
  intros n' m H IHn'.
(* We will need to consider two cases: whether n' = m and whether n' < m.
*)
  destruct (eq_nat_dec n' m) as [Heq | Hlt].
(* Case 1: n' = m *)
  {
    (* In this case, we have m = n' + 0.
*)
    exists 0.
reflexivity.
(* We also have m = S n' + 0.
*)
    exists 0.
reflexivity.
}
  (* Case 2: n' < m *)
  {
    (* We will use the induction hypothesis IHn'.
*)
    apply IHn'.
(* We have shown that there exists an x such that m = n' + x.
*)
    exists x.
reflexivity.
(* We also have m = S n' + x.
*)
    exists x.
reflexivity.
}

Qed.

