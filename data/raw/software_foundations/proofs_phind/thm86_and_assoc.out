Theorem and_assoc : forall P Q R : Prop,
  P /\ (Q /\ R) -> (P /\ Q) /\ R.

Proof.
(* The theorem and_assoc states that for any three propositions P, Q, and R,
     the proposition P /\ (Q /\ R) is equivalent to (P /\ Q) /\ R.
We will prove this by applying the proj1 and proj2 lemmas.
*)
  intros P Q R H.
(* H is the hypothesis P /\ (Q /\ R).
We first apply proj2 to H.
*)
  apply proj2 in H.
(* Now H becomes Q /\ R.
We apply proj1 to H.
*)
  apply proj1 in H.
(* Now H becomes P.
We apply and_intro to P, Q, and R.
*)
  apply and_intro.
(* The first argument of and_intro is P /\ Q, which is already proven by H.
*)
  (* The second argument of and_intro is R, which is already proven by H.
*)
  (* Therefore, the theorem and_assoc is proven.
*)

Qed.

