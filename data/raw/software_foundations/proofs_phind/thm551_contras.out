Lemma contradiction_lemma_4 : forall P : Prop, forall Hp : P, forall Hnp : ~ P, 0 = 1.

Proof.
(* The theorem states that for any proposition P, if we have a proof Hp of P and a proof Hnp of ~P (i.e., not P), then we can derive a contradiction (0 = 1).
*)
  intros P Hp Hnp.
(* We can use the law of the excluded middle to derive a proof of P or ~P.
*)
  apply (or_resolve_right (em P)).
(* If we have a proof of P, then we can apply the hypothesis Hnp to derive a contradiction.
*)
  - apply Hnp.
(* If we have a proof of ~P, then we can apply the hypothesis Hp to derive a contradiction.
*)
  - apply Hp.
(* Since we have derived a contradiction in both cases, we can conclude that 0 = 1.
*)
  reflexivity.
Qed.

