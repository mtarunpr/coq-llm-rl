Example auto_example_6 : forall n m p : nat,
  (n <= p -> (n <= m /\ m <= n)) ->
  n <= p ->
  n = m.

Proof.
(*
     We will prove auto_example_6 by induction on p.
The base case will be when p = 0, in which case we will show that n <= 0 -> n = 0.
The inductive case will be when p = S p', in which case we will show that n <= S p' -> n = S p'.
*)
  intros n m p H.
induction p as [| p' IHp'].
- (* Base case: p = 0 *)
    intros H0.
inversion H0.
(* Now we have n <= 0 and n = 0 *)
    reflexivity.
- (* Inductive case: p = S p' *)
    intros HS.
(* We will use the induction hypothesis IHp' *)
    apply IHp' in HS.
(* Now we have n <= S p' and n = S p' *)
    reflexivity.
Qed.

