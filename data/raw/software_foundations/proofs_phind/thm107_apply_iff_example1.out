Lemma logical_equivalence_implies_consequence_0 : forall H1 H2 H3 : Prop, H1 <-> H2 -> (H2 -> H3) -> H1 -> H3.

Proof.
(* We are given H1 <-> H2 and H2 -> H3.
We want to prove H1 -> H3.
We will use the fact that H1 <-> H2 implies H1 -> H2 and H2 -> H1.
Then, we can use modus ponens with H1 to get H2, and then with H2 to get H3.
*)
  intros H1 H2 H3 H1_iff_H2 H2_implies_H3 H1_implies_H3.
(* First, we prove H1 -> H2 using the iff introduction rule *)
  apply (iff_intro H1_implies_H3 H2_implies_H3).
(* Then, we use modus ponens with H1 to get H2 *)
  apply (H1_implies_H3 H1).
(* Finally, we use modus ponens with H2 to get H3 *)
  apply (H2_implies_H3 H2).
Qed.

