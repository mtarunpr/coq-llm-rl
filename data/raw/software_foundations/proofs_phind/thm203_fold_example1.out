Example fold_example1 : fold mult [1;2;3;4] 1 = 24.

Proof.
(* We will proceed by induction on the input list of fold.
*)
  intros X Y f l a.
(* We will need the following lemmas: *)
  (* 1.
mult_assoc: forall x y z, mult x (mult y z) = mult (mult x y) z *)
  (* 2.
mult_comm: forall x y, mult x y = mult y x *)
  (* 3.
mult_1_r: forall x, mult x 1 = x *)
  apply (fold_ind (fun l => forall a, fold mult l a = a)
         (fun x l IH a =>
           (* We will use the induction hypothesis IH and the lemmas *)
           (* to prove the fold mult (x :: l) a = mult x (fold mult l a).
*)
           (* We will also use the fact that mult is associative, *)
           (* so we can rewrite (mult x (fold mult l a)) as *)
           (* (fold mult (x :: l) a).
*)
           mult_assoc x (fold mult l a) a
         )
         (fun a =>
           (* The base case is trivial: fold mult [] a = a.
*)
           reflexivity
         )
         l a
  ).
Qed.

