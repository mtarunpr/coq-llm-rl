Lemma exists_zero_eq_four_plus_m_4 : forall H : exists m : nat, 0 = 4 + m, forall m : nat, forall Hm : 0 = 4 + m, 0 = S (S (m - 2)).

Proof.
(* We are given the hypothesis H that there exists an m such that 0 = 4 + m.
We want to prove that for all m, if 0 = 4 + m, then 0 = S (S (m - 2)).
We will proceed by cases on the existence of such an m.
*)
  intros H m Hm.
(* We first consider the case where m = 0.
*)
  destruct H as [m0 Hm0].
(* We then consider the case where m = S m'.
*)
  destruct m0 as [|m'].
- (* If m = 0, then Hm0 is 0 = 4 + 0, so 0 = 4.
*)
    simpl in Hm0.
(* This contradicts our assumption that 0 = 4 + m for some m, so we have a contradiction.
*)
    contradiction.
- (* If m = S m', then Hm0 is 0 = 4 + S m', so 0 = S (S (m' - 2)).
*)
    simpl in Hm0.
(* We can rewrite this as 0 = S (S (m' - 2)), which is what we wanted to prove.
*)
    reflexivity.
Qed.

