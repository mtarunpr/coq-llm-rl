- (* Case 4 = O.
*)
    (* Since 2 <= 0 is false, we know that leb 2 0 = false.
*)
    (* Therefore, the lemma holds.
*)
    reflexivity.
- (* Case 4 = S n'.
*)
    (* We will now perform case analysis on the first argument, 2.
*)
    destruct (2 : nat) as [ | n Hn].
- (* Case 2 = O.
*)
      (* Since O <= S n' is true, we know that leb O 4 = true.
*)
      (* Therefore, the lemma holds.
*)
      reflexivity.
- (* Case 2 = S n.
*)
      (* We will now perform case analysis on the second argument, S n'.
*)
      destruct (S n' : nat) as [ | m Hm].
- (* Case S n' = O.
*)
        (* Since S n <= O is false, we know that leb (S n) (S O) = false.
*)
        (* Therefore, the lemma holds.
*)
        reflexivity.
- (* Case S n' = S m.
*)
        (* We will now use the definition of leb to prove that leb (S n) (S (S m)) = true.
*)
        unfold leb in Hn.
(* We know that n <= m, and we want to prove that S n <= S (S m).
*)
        (* By the definition of leb, this is true if n <= m.
*)
        (* Therefore, the lemma holds.
*)
        reflexivity.
