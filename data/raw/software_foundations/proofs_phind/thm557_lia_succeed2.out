Lemma mult_comm_preserves_addition_1 : forall n m' : nat, forall IHm' : n * m' = m' * n, n * S m' = n + m' * n.

Proof.
(*
     We will proceed by induction on n.
The base case will be when n = 0.
The inductive case will be when n = S n'.
*)
  intros n m' IHm'.
(*
     We will now split into two cases:
     1.
n = 0
     2.
n = S n' for some n'
  *)
  destruct n as [|n'].
(*
     Case 1: n = 0
     We will show that 0 * S m' = 0 + m' * 0.
This is trivial since both sides are equal to 0.
*)
  - simpl.
reflexivity.
(*
     Case 2: n = S n'
     We will now use the induction hypothesis IHm' for n' and m'.
This gives us the equality (n' * m') = (m' * n').
We will now show that (S n' * S m') = (S n' + m' * S n').
*)
  - simpl.
rewrite IHm'.
reflexivity.
Qed.

