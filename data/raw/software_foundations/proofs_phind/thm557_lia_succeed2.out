Lemma multiplication_of_zero_0 : forall n : nat, n * 0 = 0.

Proof.
(* We will proceed by induction on n.
*)
  intros n.
(* Base case: n = 0 *)
  induction n as [|n' IHn'].
- (* n = 0 *)
    reflexivity.
- (* n = S n' *)
    (* By definition of multiplication, n * 0 = n' * 0 + 0 *)
    simpl.
(* By induction hypothesis, n' * 0 = 0 *)
    rewrite IHn'.
(* Thus, n * 0 = 0 *)
    reflexivity.
Qed.

Theorem lia_succeed2 : forall (n m : nat),
    n * m = m * n.

Proof.
intros n m.
induction m as [|m' IHm'].
- (* m = 0 *)
    simpl.
apply (@multiplication_of_zero_0 n).
- (* m = S m' *)
    simpl.
rewrite Nat.mul_succ.
rewrite IHm'.
reflexivity.
Qed.

