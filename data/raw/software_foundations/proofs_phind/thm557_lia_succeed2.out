Lemma multiplication_of_zero_0 : forall n : nat, n * 0 = 0.

Proof.
(* We will proceed by induction on n.
*)
  intros n.
(* Base case: n = 0 *)
  induction n as [|n' IHn'].
- (* n = 0 *)
    reflexivity.
- (* n = S n' *)
    (* By definition of multiplication, n * 0 = n' * 0 + 0 *)
    simpl.
(* By induction hypothesis, n' * 0 = 0 *)
    rewrite IHn'.
(* Thus, n * 0 = 0 *)
    reflexivity.
Qed.

Lemma zero_times_any_is_zero_2 : forall m' : nat, forall IHm' : 0 * m' = m' * 0, 0 = m' * 0.

Proof.
(* We will use induction on m' *)
  intros m' IHm'.
(* Base case: m' = 0 *)
  destruct m' as [|m'].
- (* m' = 0 *)
    reflexivity.
- (* m' = S m *)
    (* We will use the induction hypothesis IHm' *)
    rewrite IHm'.
(* Now we need to prove 0 = S m * 0 *)
    (* We know that S m * 0 = 0 by the definition of multiplication *)
    reflexivity.
Qed.

