Theorem eqb_neq : forall x y : nat,
  x =? y = false <-> x <> y.

Proof.
(* We will proceed by induction on x and y.
*)
  intros x y.
(* We will consider two cases: when x = y and when x <> y.
*)
  destruct (x =? y) eqn:E.
- (* Case x =? y = true *)
    simpl in E.
(* We know that x = y, so x <> y is false.
*)
    intros H.
(* We have a contradiction, so we can derive any proposition.
*)
    exfalso.
(* We derive the desired proposition: x <> y.
*)
    exact H.
- (* Case x =? y = false *)
    simpl in E.
(* We know that x <> y, so x = y is false.
*)
    intros H.
(* We have a contradiction, so we can derive any proposition.
*)
    exfalso.
(* We derive the desired proposition: x = y.
*)
    exact H.
Qed.

