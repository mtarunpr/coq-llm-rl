Lemma lemma_n_less_or_equal_m_0 : forall n m : nat, forall H : (n <=? m) = true, n <= m.

Proof.
(* We will proceed by induction on m.
*)
  intros m.
(* Base case: m = 0.
*)
  induction m as [| m' IHm'].
- (* m = 0.
*)
    intros n H.
(* We know that n <=? 0 = true.
*)
    (* This means that n = 0.
*)
    subst H.
(* We now need to prove n <= 0.
*)
    (* But this is trivial since n = 0.
*)
    apply le_n.
- (* m = S m'.
*)
    (* We will now prove the statement for m' by induction hypothesis.
*)
    intros n H.
(* We know that n <=? S m' = true.
*)
    (* This means that n = S m' or n = m'.
*)
    destruct H as [H1 | H2].
- (* n = S m'.
*)
      (* We now need to prove S n <= S m'.
*)
      (* But this is trivial since n = S m'.
*)
      apply le_S.
assumption.
- (* n = m'.
*)
      (* We now need to prove n <= m'.
*)
      (* By the induction hypothesis, we know that n <= m'.
*)
      apply IHm'.
assumption.
Qed.

