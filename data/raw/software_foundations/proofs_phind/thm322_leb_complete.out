Theorem leb_complete : forall n m,
  n <=? m = true -> n <= m.

Proof.
(* We will proceed by induction on the relation n <=? m.
*)
  intros n m H.
(* We will consider two cases: n = m and n < m.
*)
  destruct (leb_spec n m) eqn:E.
- (* Case n = m *)
    rewrite E in H.
(* We know that n =? m implies n <= m, so we can apply this fact.
*)
    apply leb_complete_eq.
- (* Case n < m *)
    (* We know that n < m implies n <= m, so we can apply this fact.
*)
    apply leb_complete_lt.
Qed.

