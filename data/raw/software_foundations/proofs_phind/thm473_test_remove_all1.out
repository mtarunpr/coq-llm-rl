Example test_remove_all1 : count 5 (remove_all 5 [2;1;5;4;1]) = 0.

Proof.
(* We will prove this by induction on the list s.
*)
  intros s.
(* We will consider two cases: the first is when s is nil, and the second is when s is a cons.
*)
  induction s as [| n s'].
(* Case 1: s is nil.
*)
  - (* In this case, the result is trivially true, as there are no occurrences of v in the empty list.
*)
    reflexivity.
(* Case 2: s is a cons.
*)
  - (* We will split into two subgoals: one where n =? v is true, and one where n =? v is false.
*)
    destruct (n =? v) as [eq_nv | neq_nv].
(* Subgoal 1: n =? v is true.
*)
    - (* In this case, we know that n is equal to v, so we can rewrite the goal to be about s'.
*)
      rewrite eq_nv in H.
(* We now know that remove_all v s = remove_all v s', so we can apply the IH to s'.
*)
      rewrite (IHs' eq_nv).
(* This gives us the desired result.
*)
      reflexivity.
(* Subgoal 2: n =? v is false.
*)
    - (* In this case, we know that n is not equal to v, so we can rewrite the goal to be about s'.
*)
      rewrite neq_nv in H.
(* We now know that remove_all v (n :: s') = n :: remove_all v s', so we can apply the IH to s'.
*)
      rewrite (IHs' neq_nv).
(* This gives us the desired result.
*)
      reflexivity.
Qed.

