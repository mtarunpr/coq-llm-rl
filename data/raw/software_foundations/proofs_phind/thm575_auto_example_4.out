Example auto_example_4 : forall P Q R : Prop,
  Q ->
  (Q -> R) ->
  P \/ (Q /\ R).

Proof.
(* We will prove the theorem by induction on the constructors of P, Q, and R.
*)
  intros P Q R H.
destruct P as [ | P_and P1 P2 | P_or P1 P2 | P_impl P1 P2].
- (* Case P = True *)
    left.
apply H.
- (* Case P = False *)
    right.
left.
apply H.
- (* Case P = And *)
    destruct Q as [ | Q_and Q1 Q2 | Q_or Q1 Q2 | Q_impl Q1 Q2].
+ (* Case Q = And *)
      right.
left.
apply H.
+ (* Case Q = Or *)
      right.
right.
left.
apply H.
+ (* Case Q = Impl *)
      right.
right.
right.
apply H.
- (* Case P = Or *)
    destruct Q as [ | Q_and Q1 Q2 | Q_or Q1 Q2 | Q_impl Q1 Q2].
+ (* Case Q = And *)
      right.
left.
apply H.
+ (* Case Q = Or *)
      right.
right.
left.
apply H.
+ (* Case Q = Impl *)
      right.
right.
right.
apply H.
- (* Case P = Impl *)
    destruct Q as [ | Q_and Q1 Q2 | Q_or Q1 Q2 | Q_impl Q1 Q2].
+ (* Case Q = And *)
      right.
left.
apply H.
+ (* Case Q = Or *)
      right.
right.
left.
apply H.
+ (* Case Q = Impl *)
      right.
right.
right.
apply H.
Qed.

