Theorem tl_length_pred : forall l:natlist,
  pred (length l) = length (tl l).

Proof.
(* We will proceed by induction on the length of the list l.
*)
  intros l.
induction l as [| n l'].
(* Base case: l = nil *)
  - simpl.
reflexivity.
(* Inductive case: l = n :: l' *)
  - simpl.
rewrite IHl'.
reflexivity.
Qed.

