Definition is_fortytwo x := (x = 42).



Lemma fortytwo_lower_bound_1 : forall x : nat, forall H : x <= 42 <= x, forall H42x : 42 <= x, is_fortytwo x.

Proof.
(*
     We will prove the lemma by induction on x.
The base case will be when x = 0.
In this case, we have H : 0 <= 42 <= 0 and H42x : 42 <= 0.
Since 42 > 0, H42x cannot hold, so we have a contradiction.
Therefore, the only way for H42x to hold is if x = 42.
*)
  intros x H H42x.
induction x as [|x' IHx'].
- (* x = 0 *)
    simpl in H.
destruct H as [H1 H2].
simpl in H2.
contradiction.
- (* x = S x' *)
    (*
       In this case, we have H : S x' <= 42 <= S x' and H42x : 42 <= S x'.
We know that x' <= 42 from H42x, so we can apply the induction hypothesis IHx'
       to conclude that is_fortytwo (S x') holds.
*)
    simpl in H42x.
apply IHx'.
assumption.
Qed.

