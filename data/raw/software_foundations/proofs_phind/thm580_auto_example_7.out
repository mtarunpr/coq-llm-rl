Definition is_fortytwo x := (x = 42).



Lemma fortytwo_property_0 : forall x : nat, forall H : x <= 42 <= x, is_fortytwo x.

Proof.
(* The theorem fortytwo_property_0 states that for any natural number x,
     if x is less than or equal to 42 and 42 is less than or equal to x, then x is 42.
We will prove this by cases on x.
*)
  intros x H.
(* We have two cases: either x = 42 or x > 42.
*)
  destruct (le_lt_dec 42 x) as [H42x | Hx42].
(* If x = 42, then the hypothesis H42x holds and we can apply it to obtain the goal.
*)
  - left; apply H42x.
(* If x > 42, then the hypothesis Hx42 holds and we can apply it to obtain the goal.
*)
  - right; apply Hx42.
Qed.

