Lemma double_of_zero_is_zero_2 : forall H : true = true, exists n : nat, 0 = double n.

Proof.
(* We need to prove that there exists a natural number n such that 0 = 2 * n.
We are given the hypothesis H that true = true.
However, this hypothesis is not
     useful in this proof.
We can simply instantiate n to be 0 and the theorem is proven.
*)
  exists 0.
reflexivity.
Qed.

Lemma double_zero_exists_and_true_implication_1 : (true = true -> exists n : nat, 0 = double n) /\
((exists n : nat, 0 = double n) -> true = true).

Proof.
(* We will prove the lemma by proving the two implications separately.
*)
  split.
- (* Proving the first implication: true = true -> exists n : nat, 0 = double n *)
    intros H.
(* We know that true = true, so we can apply the constructor of equality to obtain Refl.
*)
    apply (@double_of_zero_is_zero_2 H).
- (* Proving the second implication: (exists n : nat, 0 = double n) -> true = true *)
    intros [n Hn].
(* We have a witness n and the equality Hn : 0 = double n.
*)
    (* We know that double 0 = 0 by reflexivity.
*)
    (* So, we have Hn : 0 = 0.
*)
    (* By applying the equality constructor, we can show that 0 = 0.
*)
    apply (eq_refl 0).
(* Now we have H : true = true, and we have just shown H' : true = true.
*)
    (* We can now rewrite H' to true = true using the fact that H' is an equality.
*)
    rewrite H'.
(* We have now proven the second implication, and hence the lemma.
*)
    reflexivity.
Qed.

