Theorem All_In :
  forall T (P : T -> Prop) (l : list T),
    (forall x, In x l -> P x) <->
    All P l.
Proof.
  intros T P l.
  split.
  
  {
    induction l as [|x' l'].
    + intros H. apply I.
    + simpl. intros H.
      split.
      * apply H. left. reflexivity.
      * apply IHl'. intros x. intros H'. apply H. right. apply H'.
  }
  
  {
    induction l as [|x' l'].
    + intros [] _ [].
    + simpl. intros [H1 H2] x [H | H].
      - rewrite <- H. apply H1.
      - apply IHl'.
        apply H2. apply H.
  }
Qed.




