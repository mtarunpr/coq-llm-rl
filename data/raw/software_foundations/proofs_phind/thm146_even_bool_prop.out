Lemma double_zero_is_true_3 : forall n : nat, forall Hn : 0 = double n, true = true.

Proof.
(* The goal is to prove that for any natural number n, if 0 is equal to double n, then true is true.
We will proceed by induction on n.
*)
  intros n Hn.
(* Base case: n = 0 *)
  destruct n as [|n'].
- (* n = 0 *)
    reflexivity.
- (* n = S n' *)
    (* We know that 0 = double n, but this is impossible because double n' = S (double n') *)
    (* So, we have a contradiction.
By ex falso quodlibet, we can conclude that true = true.
*)
    apply ex_falso_quodlibet in Hn.
(* We can now conclude the proof.
*)
    reflexivity.
Qed.

