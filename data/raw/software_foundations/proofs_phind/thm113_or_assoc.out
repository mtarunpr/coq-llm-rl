Lemma disjunction_associativity_0 : forall P Q R : Prop, P \/ Q \/ R <-> (P \/ Q) \/ R.

Proof.
(* We will prove the lemma by induction on the truth values of P, Q, and R.
*)
  intros P Q R.
(* We will first consider the case where P is true.
*)
  destruct P as [P_true | P_false].
- (* If P is true, then P \/ Q \/ R is true.
*)
    left.
reflexivity.
- (* If P is false, then we need to consider the cases where Q and R are true or false.
*)
    destruct Q as [Q_true | Q_false].
- (* If Q is true, then (P \/ Q) \/ R is true.
*)
      left.
left.
reflexivity.
- (* If Q is false, then we need to consider the case where R is true or false.
*)
      destruct R as [R_true | R_false].
- (* If R is true, then (P \/ Q) \/ R is true.
*)
        left.
right.
reflexivity.
- (* If R is false, then (P \/ Q) \/ R is false.
*)
        right.
reflexivity.
(* We have proven the lemma for all possible truth values of P, Q, and R.
*)

Qed.

