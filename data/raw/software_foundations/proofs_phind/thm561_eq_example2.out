Theorem eq_example2 : forall (n m o p : nat),
    n = m ->
    o = p ->
    (n, o) = (m, p).

Proof.
(* The theorem eq_example2 is a simple equality theorem about pairs of natural numbers.
We will proceed by induction on n, and then use the equality of m and n, and the equality of o and p to show that (n, o) = (m, p).
*)
  intros n m o p Hnm Hop.
induction n as [|n' IHn'].
- (* Base case: n = 0 *)
    simpl.
rewrite Hnm in Hop.
simpl in Hop.
reflexivity.
- (* Inductive case: n = S n' *)
    simpl.
rewrite Hnm in Hop.
simpl in Hop.
rewrite <- IHn'.
reflexivity.
Qed.

