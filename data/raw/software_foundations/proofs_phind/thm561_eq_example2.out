Lemma zero_and_equality_preserve_pair_0 : forall m o p : nat, forall Hnm : 0 = m, forall Hop : o = p, (0, o) = (m, p).

Proof.
(* The goal is to prove that if 0 is equal to m and o is equal to p, then the pair (0, o) is equal to the pair (m, p).
We will proceed by applying the equality axioms and the axioms for equality of pairs.
*)
  intros m o p Hnm Hop.
(* We first rewrite the goal using the equalities Hnm and Hop.
*)
  rewrite Hnm, Hop.
(* Now, the goal is to prove that (0, o) = (0, p).
*)
  (* We apply the axiom for reflexivity of equality to prove that (0, o) = (0, o).
*)
  reflexivity.
(* We apply the axiom for substitutivity of equality for pairs to rewrite the goal to (0 = 0) and (o = p).
*)
  subst.
(* Now, the goal is to prove that 0 = 0 and o = p.
*)
  (* We apply the axiom for reflexivity of equality to prove that 0 = 0.
*)
  reflexivity.
(* Now, the goal is to prove that o = p.
*)
  (* We have already proved this by applying the axiom for substitutivity of equality for pairs.
*)
  (* Therefore, the lemma is proved.
*)

Qed.

