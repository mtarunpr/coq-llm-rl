Theorem andb_true_iff : forall b1 b2:bool,
  b1 && b2 = true <-> b1 = true /\ b2 = true.

Proof.
(* Split the biconditional theorem into two implications *)
  split.
- (* Forward direction: b1 && b2 = true -> b1 = true /\ b2 = true *)
    intros H.
(* Split the conjunction into individual goals for b1 and b2 *)
    split.
+ (* Prove b1 = true *)
      (* Apply andb_true_elim2 on the original hypothesis to deduce b1 = true *)
      destruct b1.
* (* Case b1 = true *)
        reflexivity.
* (* Case b1 = false which contradicts the hypothesis that b1 && b2 = true *)
        rewrite -> H in H.
apply (@false_neq_true_0 b2 H).
+ (* Prove b2 = true *)
      (* Apply andb_true_elim2 on the original hypothesis to deduce b2 = true *)
      destruct b1, b2.
* (* Case b1 = true, b2 = true *)
        reflexivity.
* (* Case b1 = true, b2 = false *)
        discriminate H.
* (* Case b1 = false, b2 = true *)
        reflexivity.
* (* Case b1 = false, b2 = false *)
        discriminate H.
- (* Reverse direction: b1 = true /\ b2 = true -> b1 && b2 = true *)
    intros [H1 H2].
(* Rewrite the hypotheses into the goal *)
    rewrite H1.
rewrite H2.
(* b1 and b2 are both true, so b1 && b2 simplifies to true *)
    reflexivity.
Qed.

