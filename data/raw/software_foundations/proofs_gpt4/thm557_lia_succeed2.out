Theorem lia_succeed2 : forall (n m : nat),
    n * m = m * n.

Proof.
(* We prove the theorem by induction on n.
*)
  induction n as [| n' IHn'].
- (* Case n = 0 *)
    intros m.
simpl.
rewrite Nat.mul_0_r.
reflexivity.
- (* Case n = S n' *)
    intros m.
simpl.
rewrite IHn'.
rewrite Nat.mul_succ_r.
reflexivity.
Qed.

