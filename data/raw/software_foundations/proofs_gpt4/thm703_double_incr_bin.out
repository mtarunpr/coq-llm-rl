Theorem mult_plus_distr_r : forall n m p : nat,
  (n + m) * p = (n * p) + (m * p).
Proof.
  intros n m p.
  induction p as [| p' IHp'].
  - rewrite -> mul_0_r. rewrite -> mul_0_r. rewrite -> mul_0_r.
    reflexivity.
  - rewrite <- mult_n_Sm.
