Lemma double_incr_bin : forall b,
    double_bin (incr b) = incr (incr (double_bin b)).

Proof.
(* We proceed by induction on b, the binary number *)
  intros b.
induction b as [| b' IHb' | b' IHb'].
- (* Case b = Z (base case) *)
    reflexivity.
- (* Case b = B0 b' (inductive step for B0) *)
    simpl.
(* use the induction hypothesis *)
    apply (@double_incr_bin_equiv_B0_B1_0 b' IHb').
- (* Case b = B1 b' (inductive step for B1) *)
    simpl.
(* simplifying further according to the definition of incr *)
    apply (@bin_double_incr_idempotent_1 b' IHb').
Qed.

