Theorem plus_exists_leb : forall n m, (exists x, m = n+x) -> n <=? m = true.

Proof.
intros n m H.
(* Introduce n and m and the hypothesis H into the context *)
  destruct H as [x Hx].
(* Desconstruct the existential hypothesis to get x and Hx *)
  rewrite <- Hx.
(* Rewrite the goal using the equation from Hx *)
  apply Nat.leb_le.
(* Apply the property that leb is equivalent to the <= relation *)
  apply Nat.le_add_r.
(* Use the property that adding to a number results in a greater or equal number *)

Qed.

