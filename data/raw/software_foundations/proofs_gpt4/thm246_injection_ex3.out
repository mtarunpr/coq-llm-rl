  reflexivity.
Qed.

Lemma head_equality_from_cyclic_permutation_0 : forall X : Type, forall x y z : X, forall l j : list X, forall H1 : x :: y :: l = z :: j, forall H2 : j = z :: l, x = y.

Proof.
intros X x y z l j H1 H2.
(* Use the fact that lists with equal cons must have equal heads.
*)
  inversion H1 as [H3].
(* x = z because of H1 *)
  rewrite -> H2 in H1.
(* Substitute j with z :: l in H1 *)
  inversion H1 as [H4].
(* y = z because of the new H1 *)
  rewrite <- H3 in H4.
(* Use H3 (x = z) to rewrite y to x *)
  apply (@list_head_equality_and_replication_1 X x y z l j H1 H2 H3 H H4 H0).
Qed.

Example injection_ex3 : forall (X : Type) (x y z : X) (l j : list X),
  x :: y :: l = z :: j ->
  j = z :: l ->
  x = y.

Proof.
(* Introduce all variables into the context.
*)
  intros X x y z l j.
(* Introduce the first hypothesis, which states that two lists are equal.
*)
  intros H1 H2.
(* Applying the injectivity of the cons constructor to the first hypothesis.
*)
  apply (@head_equality_from_cyclic_permutation_0 X x y z l j H1 H2).
Qed.

