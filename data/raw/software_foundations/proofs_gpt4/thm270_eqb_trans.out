Theorem eqb_trans : forall n m p,
  n =? m = true ->
  m =? p = true ->
  n =? p = true.

Proof.
intros n m p H1 H2.
(* Apply eqb_true theorem, which says that if `n =? m` is true, then `n = m` *)
  apply eqb_true in H1.
(* Apply eqb_true theorem, which says that if `m =? p` is true, then `m = p` *)
  apply eqb_true in H2.
(* Rewrite the goal using the transitivity of equality *)
  rewrite H1 in H2.
(* Apply eqb_true theorem, this time in the forward direction, as we proved `n = p` *)
  apply eqb_true.
(* Now that the goal is `n = p`, we conclude by the transitivity of equality *)
  apply H2.
Qed.

