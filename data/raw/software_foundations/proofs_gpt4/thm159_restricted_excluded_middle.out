Lemma contradiction_from_false_implies_true_6 : forall P : Prop, forall b : bool, forall E : b = false, forall H1 : P -> false = true, forall H2 : false = true -> P, forall Hc : true = true, forall H : false = true, False.

Proof.
(* Introduce all premises to the context *)
  intros P b E H1 H2 Hc H.
(* Use H: false = true to derive P using H2 *)
  apply H2 in H.
(* Apply H1 with P to derive a contradiction *)
  apply H1 in H.
(* Now we have both false = true and its negation, hence a contradiction *)
  discriminate H.
Qed.

Lemma contradiction_from_false_implies_true_5 : forall P : Prop, forall b : bool, forall E : b = false, forall H1 : P -> false = true, forall H2 : false = true -> P, forall Hc : false = true, False.

Proof.
(* Introduce all hypotheses into the proof context *)
  intros P b E H1 H2 Hc.
(* Apply hypothesis H2 with Hc to get P *)
  apply H2 in Hc as H.
(* Apply hypothesis H1 with H to get false = true, which contradicts Hc *)
  apply H1 in H.
(* Using H (false = true) and Hc (false = true), we derive a contradiction *)
  rewrite H in Hc.
(* we have true = true from Hc which is trivially reflexive, but our goal is False *)
  apply (@contradiction_from_false_implies_true_6 P b E H1 H2 Hc H).
Qed.

