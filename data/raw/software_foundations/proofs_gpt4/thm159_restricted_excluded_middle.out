Lemma contradiction_from_false_equality_6 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> false = true, forall Hc : false = true, False.

Proof.
intros P b E Himpl Hc.
(* Introduce all the variables and hypotheses into the context *)
  rewrite Hc in Himpl.
(* Replace false with true in the hypothesis Himpl using Hc, which leads to a contradiction *)
  exfalso.
(* Derive a contradiction (ex falso quodlibet) *)
  apply (@contradiction_from_false_implies_true_7 P b E Himpl Hc).
Qed.

Lemma contradiction_from_false_equality_5 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> false = true, forall Hc : false = true, False.

Proof.
(* we introduce all hypotheses *)
  intros P b E Himpl Hc.
(* Apply Himpl to derive false = true from P, but since Himpl requires P, we use ex_falso_quodlibet to derive P from false - a contradiction *)
  apply (@contradiction_from_false_equality_6 P b E Himpl Hc).
Qed.

Lemma false_implies_no_contradictory_evidence_4 : forall P : Prop, forall b : bool, forall E : b = false, forall H : P <-> false = true, forall Hc : P, False.

Proof.
intros P b E H Hc.
(* Introduce all premises *)
  (* From H, we have P implies false = true *)
  destruct H as [Himpl _].
(* Destruct the bi-implication, we only need the forward direction *)
  (* Apply the forward implication to Hc to get false = true, which is a contradiction *)
  apply Himpl in Hc.
(* Apply the implication to get false = true *)
  apply (@contradiction_from_false_equality_5 P b E Himpl Hc).
Qed.

Lemma false_equiv_true_implies_False_3 : forall P : Prop, forall b : bool, forall E : b = false, forall H : P <-> false = true, forall Hc : false = true, False.

Proof.
intros P b E H Hc.
(* Since H is an equivalence, we can use it to substitute 'false = true' with P *)
  apply H in Hc.
(* But we have an assumption E that b = false.
Since b is of type bool and can only be true or false,
     having b = false rules out the possibility of b = true, effectively making 'false = true' an absurdity.
*)
  apply (@false_implies_no_contradictory_evidence_4 P b E H Hc).
Qed.

Lemma false_implies_no_contradictory_evidence_2 : forall P : Prop, forall b : bool, forall E : b = false, forall H : P <-> false = true, forall Hc : P, False.

Proof.
intros P b E H Hc.
rewrite H in Hc.
apply (@false_equiv_true_implies_False_3 P b E H Hc).
Qed.

Lemma contradiction_from_false_implies_true_6 : forall P : Prop, forall b : bool, forall E : b = false, forall H1 : P -> false = true, forall H2 : false = true -> P, forall Hc : true = true, forall H : false = true, False.

Proof.
(* Introduce all premises to the context *)
  intros P b E H1 H2 Hc H.
(* Use H: false = true to derive P using H2 *)
  apply H2 in H.
(* Apply H1 with P to derive a contradiction *)
  apply H1 in H.
(* Now we have both false = true and its negation, hence a contradiction *)
  discriminate H.
Qed.

