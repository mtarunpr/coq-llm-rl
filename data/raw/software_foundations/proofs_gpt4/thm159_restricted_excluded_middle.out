  - 
    induction l as [|x'].
    + simpl. intros []. apply I.
    + simpl. destruct (test x').
      { simpl. intros H. split. reflexivity. apply IHl in H. apply H. }
      { simpl. discriminate. }
  - 
    induction l as [|x'].
    + simpl. reflexivity.
    + simpl. destruct (test x').
      { simpl. intros [_ H]. apply IHl in H. apply H. }
      { simpl. intros [H _]. discriminate H. }
Qed.










