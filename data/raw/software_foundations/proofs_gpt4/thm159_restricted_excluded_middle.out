Lemma contradiction_from_false_implies_true_8 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> true = true, forall Hc : false = true, False.

Proof.
(* Introduce all variables into the context *)
  intros P b E Himpl Hc.
(* The assumption Hc: false = true contradicts Coq's built-in discrimination of true and false, thus we can use discriminate to derive False *)
  discriminate Hc.
