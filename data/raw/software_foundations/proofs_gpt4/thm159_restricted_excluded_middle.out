Fixpoint eqb_list {A : Type} (eqb : A -> A -> bool)
                  (l1 l2 : list A) : bool
  := match l1, l2 with
     | [], [] => true
     | _, [] => false
     | [], _ => false
     | (x1 :: l1'), (x2 :: l2') => eqb x1 x2 && eqb_list eqb l1' l2'
     end.

Theorem eqb_list_true_iff :
  forall A (eqb : A -> A -> bool),
    (forall a1 a2, eqb a1 a2 = true <-> a1 = a2) ->
    forall l1 l2, eqb_list eqb l1 l2 = true <-> l1 = l2.
Proof.
  intros A eqb.
  intros H.
  split.
  - 
    generalize dependent l2.
    induction l1.
    + destruct l2. reflexivity. discriminate.
    + destruct l2. discriminate.
      simpl.
      intros H'. rewrite andb_true_iff in H'. destruct H'.
      rewrite H in H0. apply IHl1 in H1. rewrite H0. rewrite H1. reflexivity.
  - 
    generalize dependent l2.
    induction l1.
    + intros l2. intros H2. rewrite <- H2. reflexivity.
    + destruct l2. discriminate.
      simpl. intros H'. injection H' as Hx Hl. rewrite andb_true_iff. split.
      { rewrite Hx. rewrite H. reflexivity. }
      { apply IHl1. apply Hl. }
Qed.






Fixpoint forallb {X : Type} (test : X -> bool) (l : list X) : bool
  := match l with
     | [] => true
     | x :: l' => andb (test x) (forallb test l')
     end.

Theorem forallb_true_iff : forall X test (l : list X),
  forallb test l = true <-> All (fun x => test x = true) l.
Proof.
  intros X test l.
  split.
  - 
    induction l as [|x'].
    + simpl. intros []. apply I.
    + simpl. destruct (test x').
      { simpl. intros H. split. reflexivity. apply IHl in H. apply H. }
      { simpl. discriminate. }
  - 
    induction l as [|x'].
    + simpl. reflexivity.
    + simpl. destruct (test x').
      { simpl. intros [_ H]. apply IHl in H. apply H. }
      { simpl. intros [H _]. discriminate H. }
Qed.










Definition excluded_middle := forall P : Prop,
  P \/ ~ P.







Lemma contradiction_from_false_implies_true_8 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> true = true, forall Hc : false = true, False.

Proof.
(* Introduce all variables into the context *)
  intros P b E Himpl Hc.
(* The assumption Hc: false = true contradicts Coq's built-in discrimination of true and false, thus we can use discriminate to derive False *)
  discriminate Hc.
Qed.

Lemma contradiction_from_false_implies_true_7 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> true = true, forall Hc : false = true, False.

Proof.
intros P b E Himpl Hc.
apply (@contradiction_from_false_implies_true_8 P b E Himpl Hc).
Qed.

Lemma contradiction_from_false_equality_6 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> false = true, forall Hc : false = true, False.

Proof.
intros P b E Himpl Hc.
(* Introduce all the variables and hypotheses into the context *)
  rewrite Hc in Himpl.
(* Replace false with true in the hypothesis Himpl using Hc, which leads to a contradiction *)
  exfalso.
(* Derive a contradiction (ex falso quodlibet) *)
  apply (@contradiction_from_false_implies_true_7 P b E Himpl Hc).
Qed.

Lemma contradiction_from_false_equality_5 : forall P : Prop, forall b : bool, forall E : b = false, forall Himpl : P -> false = true, forall Hc : false = true, False.

Proof.
(* we introduce all hypotheses *)
  intros P b E Himpl Hc.
(* Apply Himpl to derive false = true from P, but since Himpl requires P, we use ex_falso_quodlibet to derive P from false - a contradiction *)
