  split.
  + intros H.
    destruct (x =? y) eqn:H1.
    - discriminate H.
    - intros contra.
      rewrite <- eqb_eq in contra. rewrite H1 in contra. discriminate contra.
  + intros H.
    destruct (x =? y) eqn:H1.
    - rewrite <- eqb_eq in H. apply H in H1. destruct H1.
