Theorem In_map :
  forall (A B : Type) (f : A -> B) (l : list A) (x : A),
         In x l ->
         In (f x) (map f l).
Proof.
  intros A B f l x.
  induction l as [|x' l' IHl'].
  - 
    simpl. intros [].
  - 
    simpl. intros [H | H].
    + rewrite H. left. reflexivity.
    + right. apply IHl'. apply H.
Qed.




Theorem In_map_iff :
  forall (A B : Type) (f : A -> B) (l : list A) (y : B),
         In y (map f l) <->
         exists x, f x = y /\ In x l.
Proof.
  intros A B f l y. split.
  - 
    induction l as [| x' l'].
    + simpl. intros [].
    + simpl. intros [H | H].
      * 
        exists x'. split. apply H. left. reflexivity.
      * 
        apply IHl' in H. destruct H as [x [H1 H2]].
        exists x. split. apply H1. right. apply H2.
  - 
    intros [x [H1 H2]]. rewrite <- H1. apply In_map. apply H2.
Qed.



Theorem In_app_iff : forall A l l' (a:A),
  In a (l++l') <-> In a l \/ In a l'.
Proof.
  intros A l. induction l as [|a' l' IH].
  + split. right. apply H. intros [H | H]. destruct H. apply H.
  + split.
    - intros [H | H].
      * left. left. apply H.
      * simpl. apply or_assoc. right. apply IH. apply H.
    - intros [H | H].
      * destruct H as [H' | H'].
        left. apply H'.
        right. apply IH. left. apply H'.
      * right. apply IH. right. apply H.
Qed.




Fixpoint All {T : Type} (P : T -> Prop) (l : list T) : Prop
  := match l with
     | [] => True
     | (x :: l') => P x /\ All P l'
     end.



Theorem All_In : forall T (P : T -> Prop) (l : list T),
    (forall x, In x l -> P x) <->
    All P l.

Proof.
intros T P l.
split.
- (* -> (from universal quantification to `All`) *)
    (* We assume that P applies to every element in the list and prove that `All P l` holds.
*)
    intros H.
induction l as [|x l' IHl'].
+ (* Case l = []: The base case is immediate because `Forall` over an empty list is trivially true.
*)
      apply Forall_nil.
+ (* Case l = x :: l': We apply `Forall_cons` to show `P x` and `All P l'`.
*)
      apply Forall_cons.
* (* We prove `P x` using the assumption for the head `x`.
*)
        apply H.
simpl.
left.
reflexivity.
* (* We prove `All P l'` using the induction hypothesis.
*)
        apply IHl'.
intros y Hin.
apply H.
simpl.
right.
assumption.
- (* <- (from `All` to universal quantification) *)
    (* We assume `All P l` and prove that P applies to every element in the list.
*)
    intros Hall x Hin.
(* We use `Forall_forall` lemma which states the equivalence of `Forall` and 
       universal quantification over list elements based on `In`.
*)
    apply Forall_forall with (x:=x) in Hall.
apply Hall.
assumption.
Qed.

