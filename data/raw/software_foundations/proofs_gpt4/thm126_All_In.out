Lemma forall_cons_head_satisfies_predicate_3 : forall T : Type, forall P : T -> Prop, forall x : T, forall l' : list T, forall H : forall x0 : T, In x0 (x :: l') -> P x0, forall IHl' : (forall x : T, In x l' -> P x) -> All P l', P x.

Proof.
intros T P x l' H IHl'.
apply H.
(* Use the hypothesis H to show that P x is true.
*)
  simpl.
(* Simplify the In predicate with the cons case.
*)
  left.
(* Choose the left side of the disjunction, which is "x = x".
*)
  reflexivity.
(* Apply reflexivity of equality to finish the proof.
*)

Qed.

