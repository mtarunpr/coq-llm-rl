Lemma true_for_empty_list_1 : forall T : Type, forall P : T -> Prop, forall H : forall x : T, In x [ ] -> P x, True.

Proof.
(* This is trivial because True is always provable, regardless of the type T, the property P, or the hypothesis H.
*)
  intros T P H.
(* Introducing all universal quantifiers.
*)
  exact I.
(* Concluding the proof with the constructor for True.
*)

Qed.

