Lemma MUnion' : forall T (s : list T) (re1 re2 : reg_exp T),
  s =~ re1 \/ s =~ re2 ->
  s =~ Union re1 re2.

Proof.
(* Introduce variables into context and proceed by case analysis on the disjunction *)
  intros T s re1 re2 [H1 | H2].
- (* Case when s matches re1 *)
    apply MUnionL.
assumption.
- (* Case when s matches re2 *)
    apply MUnionR.
assumption.
Qed.

