  - reflexivity.
  - exfalso.
    rewrite <- H0 in H2.
    assert (Nonsense: S m <= m). {
      apply le_trans with a.
      apply H2.
      apply H.
    }
    apply (le_Sn_n m Nonsense).
Qed.



Theorem le_step : forall n m p,
  n < m ->
  m <= S p ->
  n <= p.
Proof.
